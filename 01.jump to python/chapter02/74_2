stack_dishes=[]
print(stack_dishes)
stack_dishes.append("d1")
print(stack_dishes)
stack_dishes.append("d2")
print(stack_dishes)
stack_dishes.append("d3")
print(stack_dishes)
stack_dishes.append("d4")
print(stack_dishes)

stack_dishes.pop(1)
print(stack_dishes)
stack_dishes.pop(2)
print(stack_dishes)
stack_dishes.pop(3)
print(stack_dishes)
stack_dishes.pop(4)
print(stack_dishes)
